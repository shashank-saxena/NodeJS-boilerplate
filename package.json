{
  "name": "nodejs-boilerplate",
  "version": "1.0.0",
  "description": "sample structure to use in node js projects",
  "main": "index.js",
  "engines": {
    "node": "=8.11.1",
    "npm": "=5.8.0"
  },
  "private": true,
  "scripts": {
    "flow": "flow",
    "flow:check": "flow check ./src/",
    "check-versions": "node ./validate-environment",
    "dev": "npm run check-versions && NODE_ENV=development node --inspect index",
    "stage": "npm run check-versions && NODE_ENV=staging node index",
    "prestart": "npm run check-versions",
    "start": "node index",
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github-newput:shashank-saxena/NodeJS-boilerplate.git"
  },
  "keywords": [
    "nodejs",
    "boilerplate"
  ],
  "author": "",
  "license": "MIT"
}
